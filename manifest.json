{
    "manifest_version": 3,
    "name": "Replace Letters A to number",
    "version": "1.1",
    "description": "Replace Letters A to number",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "icons": {
        "16": "icons/icons8-bookmark-16.png",
        "48": "icons/icons8-bookmark-48.png",
        "128": "icons/icons8-bookmark-128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icons8-bookmark-16.png",
            "48": "icons/icons8-bookmark-48.png",
            "128": "icons/icons8-bookmark-128.png"
        },
        "default_title": ""
    }
}
